{
    "name": "template-react-router-v7",
    "description": "A Phaser 3 React Router project template that demonstrates React Router v7 with React communication and uses Vite for bundling.",
    "sideEffects": false,
    "version": "1.1.0",
    "type": "module",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/phaserjs/template-remix.git"
    },
    "author": "Phaser Studio <support@phaser.io> (https://phaser.io/)",
    "license": "MIT",
    "licenseUrl": "http://www.opensource.org/licenses/mit-license.php",
    "bugs": {
        "url": "https://github.com/phaserjs/template-remix/issues"
    },
    "homepage": "https://github.com/phaserjs/template-remix#readme",
    "keywords": [
        "phaser",
        "phaser3",
        "react-router",
        "vite",
        "typescript"
    ],
    "scripts": {
        "dev": "node log.js dev & react-router dev",
        "dev-nolog": "react-router dev",
        "build": "node log.js build & react-router build",
        "build-nolog": "react-router build",
        "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
        "start": "serve -s build/client -l 3000",
        "typecheck": "react-router typegen && tsc"
    },
    "dependencies": {
        "@react-router/fs-routes": "^7.7.0",
        "@react-router/node": "^7.7.0",
        "isbot": "^5.1.28",
        "phaser": "^3.90.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-router": "^7.0.0"
    },
    "devDependencies": {
        "@react-router/dev": "^7.0.0",
        "@types/node": "^24.0.14",
        "@types/react": "^18.2.20",
        "@types/react-dom": "^18.2.7",
        "@typescript-eslint/eslint-plugin": "^6.7.4",
        "@typescript-eslint/parser": "^6.7.4",
        "eslint": "^8.38.0",
        "eslint-import-resolver-typescript": "^3.6.1",
        "eslint-plugin-import": "^2.28.1",
        "eslint-plugin-jsx-a11y": "^6.7.1",
        "eslint-plugin-react": "^7.33.2",
        "eslint-plugin-react-hooks": "^4.6.0",
        "serve": "^14.2.4",
        "typescript": "^5.1.6",
        "vite": "^5.1.0",
        "vite-tsconfig-paths": "^4.2.1"
    },
    "engines": {
        "node": ">=20.0.0"
    }
}
